 urldate = {2020-12-15},
 abstract = {Piano wire, or {\textquotedbl}music wire{\textquotedbl}, is a specialized type of wire made for use in piano strings but also in other applications as springs. It is made from tempered high-carbon steel, also known as spring steel, which replaced iron as the material starting in 1834.

